<br />

<div class="booyah-box col-10 offset-1">

  <h1>Add a new trip</h1>
  <%= simple_form_for @trip, url: "#", html: {id: 'newTripForm' }  do |f| %>
    <div class="booyah-box">
      <div class="form-inputs">
        <%= f.input :description %>
        <%= f.input :address %>
        <br />
        <%= f.submit 'Submit', class: 'btn btn-primary' %>
      </div>
    </div>
  <% end %>
</div>
